        "@types/react": "^18.2.41",
        "@types/react-slick": "^0.23.13",
        "@types/supertest": "^2.0.16",
        "autoprefixer": "^10.4.16",
        "nodemon": "^3.1.0",
        "postcss": "^8.4.32",
        "rimraf": "^5.0.5",
        "serve-static": "^2.0.0-beta.2",
        "supertest": "^6.3.3",
        "tailwindcss": "^3.3.6",
        "ts-node": "^10.9.2",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3",
        "unplugin-swc": "^1.4.4",
        "vite": "^5.0.4",
        "vite-plugin-live-reload": "^3.0.3",
        "vitest": "^0.34.6"
    }
}
