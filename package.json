{
    "name": "@vigilio/express",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "description": "create web apps with @vigilio/express now",
    "scripts": {
        "dev": "bun --watch app/main.ts --exec app/main.tsx",
        "build:client": "vite build --outDir dist/app/client",
        "build:serve": "tsc -p tsconfig.server.json && vite build --ssr app/services/entry-server.tsx --outDir dist/entry",
        "build": "rimraf dist && bun run build:serve && bun run build:client",
        "start": "NODE_ENV=production bun ./dist/app/main.js",
        "test": "vitest",
        "biome:format": "biome format  ./app --write",
        "biome:check": "biome check ./app"
    },
    "keywords": [
        "@vigilio/express",
        "@vigilio/express-api",
        "bun"
    ],
    "author": "Vigilio",
    "license": "ISC",
    "dependencies": {
        "@decorators/di": "^2.0.0",
        "@decorators/express": "^2.9.4",
        "@hookform/resolvers": "^3.3.3",
        "@loadable/component": "^5.16.3",
        "@preact/signals": "^1.2.2",
        "@preact/signals-react": "^2.0.1",
        "@types/react-dom": "^18.2.22",
        "@vigilio/express-core": "^1.7.18",
        "@vigilio/preact-fetching": "^2.0.7",
        "@vigilio/preact-paginator": "^0.0.8",
        "@vigilio/preact-table": "^1.0.16",
        "@vigilio/sweet": "^1.0.11",
        "@vitejs/plugin-react": "^4.2.1",
        "alpinejs": "^3.13.3",
        "aos": "^2.3.4",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "formidable": "^3.5.1",
        "js-cookie": "^3.0.5",
        "memorystore": "^1.6.7",
        "million": "^2.6.4",
        "module-alias": "^2.2.3",
        "morgan": "^1.10.0",
        "passport": "^0.7.0",
        "passport-local": "^1.0.0",
        "pg": "^8.11.3",
        "preact": "^10.19.2",
        "preact-render-to-string": "^6.4.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^2.0.4",
        "react-hook-form": "^7.48.2",
        "react-router-dom": "^6.22.3",
        "react-slick": "^0.29.0",
        "sequelize": "^6.35.1",
        "sequelize-typescript": "^2.1.6",
        "sharp": "0.32.6",
        "slick-carousel": "^1.8.1",
        "tslib": "^2.6.2",
        "valibot": "^0.20.1",
        "wouter-preact": "^2.12.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.4.1",
        "@faker-js/faker": "^8.3.1",
        "@preact/preset-vite": "^2.7.0",
        "@types/alpinejs": "^3.13.5",
        "@types/aos": "^3.0.7",
        "@types/bun": "^1.0.2",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.6",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.17.10",
        "@types/formidable": "^3.4.5",
        "@types/js-cookie": "^3.0.6",
        "@types/loadable__component": "^5.13.9",
